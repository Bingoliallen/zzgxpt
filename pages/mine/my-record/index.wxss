.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    padding-left:30rpx;
    padding-right:30rpx;
    padding-bottom: 50px;
  } .newsList-record {
    width: 100%;
    padding-top:0rpx;
    padding-bottom:0rpx;
    border-bottom: 8px solid #f0f0f0;
    display: flex;
    flex-direction: column;
  }
  .cover {
    width: 100%;
  }
  .cover-img-record {
    width: 100%;
    height: 80px;
    border-radius: 10px;
  }
  .title-record {
    font-size: 13px;
    margin-top: 10px;
    margin-bottom: 10px;
  }
  .descript {
    margin-top: 0px;
    color:#666;
  }
  .descript-bl {
    margin-top: 0px;
    color:#0081ff;
  }
  
  
  .miaosha-price-btn {
    width: 100%;
    padding-left:0rpx;
    padding-right:0rpx;
    padding-bottom: 10px;
    padding-top:10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
  }